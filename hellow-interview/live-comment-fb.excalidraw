    {
      "id": "7CCA-rSfJg32k21CVc9Uc",
      "type": "text",
      "x": 5166.153574873679,
      "y": -13642.49782299239,
      "width": 951.5666854381561,
      "height": 280,
      "angle": 0,
      "strokeColor": "#1e1e1e",
      "backgroundColor": "transparent",
      "fillStyle": "solid",
      "strokeWidth": 2,
      "strokeStyle": "solid",
      "roughness": 1,
      "opacity": 100,
      "groupIds": [],
