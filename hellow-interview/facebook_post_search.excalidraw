    {
      "id": "VsL-8K8WKmCsZeTCCzJRD",
      "type": "text",
      "x": 393.19763051672203,
      "y": 1277.9241551986963,
      "width": 845.2996215820312,
      "height": 425,
      "angle": 0,
      "strokeColor": "#1e1e1e",
      "backgroundColor": "transparent",
      "fillStyle": "solid",
      "strokeWidth": 2,
      "strokeStyle": "solid",
      "roughness": 1,
      "opacity": 100,
      "groupIds": [],
