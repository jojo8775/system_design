    {
      "id": "Hb_7A2ORZTMIfieyLGQBj",
      "type": "text",
      "x": 2102.0546093382936,
      "y": 1121.7378659126116,
      "width": 1477.35888671875,
      "height": 650,
      "angle": 0,
      "strokeColor": "#1e1e1e",
      "backgroundColor": "transparent",
