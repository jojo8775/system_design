      "type": "text",
      "x": 3877.7097910195894,
      "y": 3024.611203249619,
      "width": 628.6875,
      "height": 367.20000000000005,
      "angle": 0,
      "strokeColor": "#1e1e1e",
      "backgroundColor": "transparent",
